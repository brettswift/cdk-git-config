{
  "name": "cdk-git-config",
  "version": "0.4.2",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "author": "brettswift@gmail.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brettswift/cdk-git-config.git"
  },
  "scripts": {
    "preversion": "npm test",
    "postversion": "git push --follow-tags origin master",
    "release-patch": "standard-version -r patch && npm run postversion",
    "release-minor": "standard-version -r minor && npm run postversion",
    "release-major": "standard-version -r major && npm run postversion",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "npm run build; jest",
    "cdk": "cdk"
  },
  "keywords": [
    "aws",
    "cdk",
    "ssm",
    "git"
  ],
  "peerDependencies": {
    "@aws-cdk/core": "^1.119.0"
  },
  "devDependencies": {
    "@types/node": "^16.7.1",
    "aws-cdk": "^1.119.0",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@aws-cdk/assert": "^1.119.0",
    "@aws-cdk/aws-ssm": "^1.119.0",
    "@aws-cdk/core": "^1.119.0",
    "@types/flat": "5.0.2",
    "@types/glob": "^7.1.4",
    "@types/jest": "^27.0.1",
    "@types/js-yaml": "^4.0.3",
    "flat": "^5.0.2",
    "glob": "^7.1.7",
    "jest": "^27.0.6",
    "js-yaml": "^4.1.0",
    "source-map-support": "^0.5.19"
  }
}
