    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@aws-cdk/assert": "^2.36.0",
    "@types/flat": "5.0.2",
    "@types/glob": "^7.2.0",
    "@types/jest": "^28.1.6",
    "@types/js-yaml": "^4.0.5",
    "aws-cdk-lib": "^2.36.0",
    "constructs": "^10.1.71",
