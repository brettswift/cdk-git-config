{
  "compilerOptions": {
    "target":"ES2019",
    "module": "commonjs",
    "lib": ["es2019", "es2019.object", "es2019.string", "ES2019"],
    "declaration": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization":false,
    "typeRoots": ["./node_modules/@types"],
  },
  "exclude": [
      "cdk.out",
      "dist"
  ]
}
